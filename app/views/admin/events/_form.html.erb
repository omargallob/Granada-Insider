<div style="float:left">
 <p>
   <%= f.label :name %><br />
   <%= f.text_field :name %>
 </p>
 <p>
   <%= f.label :title %><br />
   <%= f.text_field :title %>
 </p>
 <p>
   <%= f.label :location_id %><br />
   <%= f.select :location_id, Location.find(:all).collect{|q| [q.title,q.id]}, :include_blank => true %>
 </p>
 <p>
   <%= f.label :two_liner %><br />
   <%= f.text_area :two_liner %>
 </p>
 <p>
   <%= f.label :description %><br />
   <%= f.text_area :description %>
 </p>
</div>
<div style="float:left;margin-right:150px">
<p>
	<%= f.label(:start_at) %><br />
	<%= f.date_select(:start_at) %>
</p>	
<p>
	<%= f.label(:end_at) %><br />
	<%= f.date_select(:end_at) %>
</p>	
</div>
