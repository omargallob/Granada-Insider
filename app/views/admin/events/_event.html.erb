<tr>
	<td style="padding:0 10px 0 0"><%= event.event_type.title if event.event_type %></td>
	<td class="title"><%= event.title %></td>
	<td style="padding:0 10px 0 0"><%= event.location.title if event.location %></td>
	<td><%= event.start_at.strftime("%a %d %B %Y") %></td>
	<td style="width:15px"></td>
	<td><%= event.state  %></td>
	<td style="width:15px"></td>
	<td>
		<table class="nolines">
			<tr>
				<td><%= link_to image_tag ('system/admin/buttons/search.jpg', :height => "15px", :title => "show"), admin_event_path(event) %></td><td>&nbsp;|&nbsp;</td>
				<td><%= link_to image_tag ('system/admin/buttons/edit.jpg', :height => "15px", :title => "edit"), edit_admin_event_path(event) %></td><td>&nbsp;|&nbsp;</td>
				<td><%= link_to image_tag ('system/admin/buttons/delete.jpg', :height => "15px", :title => "delete"), admin_event_path(event), :confirm => 'Are you sure?', :method => :delete %></td>
			</tr>
		</table>	
	</td>
</tr>
